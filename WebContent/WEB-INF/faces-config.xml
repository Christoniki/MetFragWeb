<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">

    <application>
        <locale-config>
            <default-locale>en</default-locale>
        </locale-config>
    </application>
	<managed-bean>
		<managed-bean-name>metFragBean</managed-bean-name>
		<managed-bean-class>de.ipbhalle.metfrag.web.controller.MetFragBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pictureCleaner</managed-bean-name>
		<managed-bean-class>de.ipbhalle.metfrag.web.controller.PictureCleaner</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>popupBean</managed-bean-name>
		<managed-bean-class>de.ipbhalle.metfrag.web.controller.PopupBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>mzAnnotateBean</managed-bean-name>
		<managed-bean-class>de.ipbhalle.metfrag.web.controller.MzAnnotateBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>resultRowManager</managed-bean-name>
		<managed-bean-class>de.ipbhalle.metfrag.web.controller.ResultRowManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>styleBean</managed-bean-name>
		<managed-bean-class>de.ipbhalle.metfrag.web.controller.StyleBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
</faces-config>
